val add_i : int -> int -> int
(** [add_i a b] is [a] added to [b] *)

val sub_i : int -> int -> int
(** [sub_i a b] is [a] - [b] *)

val mult_i : int -> int -> int
(** [mult_i a b] is [a] * [b]*)

val div_i : int -> int -> int
(** [div_i a b] is integer division [a] / [b]*)

val add_f : float -> float -> float
(** [add a b] is [a] added to [b] *)

val sub_f : float -> float -> float
(** [sub a b] is [a] - [b] *)

val mult_f : float -> float -> float
(** [mult a b] is [a] * [b]*)

val div_f : float -> float -> float
(** [div a b] is float division [a] / [b]*)

val inverse : float -> float
(** [inverse a] is 1 / [a]*)

val square : float -> float
(** [square a] is [a]^2*)

val sqrt : float -> float
(** [sqrt a] is [a]^0.5*)

val log : float -> float
(** [log a] is log base 10 [a]*)

val ten_x : float -> float
(** [ten_x a] is 10^[a]*)

val ln : float -> float
(** [ln a] is natural log of [a]*)

val exp : float -> float
(** [exp a] is e^[a]*)

val pi : float
(** [pi] is 3.1415... or Pi *)

val e : float
(** [e] is 2.718... or e*)
